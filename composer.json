    "homepage": "https://github.com/darookee/u2f-two-factor-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nils Uliczka",
            "email": "nils.uliczka@darookee.net"
        }
    ],
    "require": {
        "scheb/two-factor-bundle": "^1.4 | ^2.0",
        "yubico/u2flib-server": "^0.1.0 | ^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "R\\U2FTwoFactorBundle\\": ""
        }
    }
}
